{
  "manifest_version": 2,

  "name": "shush",
  "version": "1.0",
  "description": "An easy way to block pesky videos based upon keywords.",
  "permissions": [
    "storage"
  ],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "content_scripts": [
    {
      "css": [
        "src/style.css"
      ],
      "js": [
        "src/content.js"
      ],
      "matches": [
        "https://www.youtube.com/*"
      ]
    }]

}