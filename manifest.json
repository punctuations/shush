{
  "manifest_version": 2,

  "name": "shush",
  "version": "0.1.0",
  "description": "An easy way to block pesky videos based upon keywords.",

  "content_scripts": [
    {
      "css": [
        "style.css"
      ],
      "js": [
        "content.js"
      ],
      "matches": [
        "https://www.youtube.com/*"
      ]
    }]

}